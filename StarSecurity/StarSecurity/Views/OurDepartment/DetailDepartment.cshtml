@model StarSecurity.Entites.Department
@{
    ViewData["Title"] = Model.Name;
}

<div class="content-wrapper">
    <div class="container">
        <div class="title">
            <h3>
                @Model.Name
            </h3>
        </div>
        <div class="content">
            <p>
                @Html.Raw(Model.Description)
            </p>
        </div>
    </div>
</div>